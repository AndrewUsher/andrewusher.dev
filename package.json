    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "babel-eslint": "10.1.0",
    "c8": "^7.11.3",
    "concurrently": "7.1.0",
    "eslint": "8.11.0",
    "eslint-plugin-jest": "26.1.2",
    "jsdom": "19.0.0",
    "pino-pretty": "7.6.1",
    "tailwindcss": "3.0.23",
    "ts-node": "10.7.0",
