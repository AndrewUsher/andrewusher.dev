    "babel-eslint": "10.1.0",
    "babel-jest": "27.4.6",
    "concurrently": "7.0.0",
    "eslint": "7.32.0",
    "eslint-plugin-jest": "26.0.0",
    "jest": "27.4.7",
    "tailwindcss": "3.0.10",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
