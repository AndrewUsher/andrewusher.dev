{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "yarn build:css && remix build",
    "build:css": "tailwindcss -i ./app/styles/tailwind.css -o ./app/tailwind.css --minify",
    "dev": "concurrently \"yarn dev:css\" \"node -r dotenv/config node_modules/.bin/remix dev\"",
    "dev:css": "tailwindcss -i ./app/styles/tailwind.css -o ./app/tailwind.css --watch",
    "lint": "eslint app/",
    "postinstall": "remix setup node",
    "start": "remix start",
    "test": "vitest run"
  },
  "dependencies": {
    "@giscus/react": "2.0.3",
    "@heroicons/react": "^1.0.6",
    "@jsdevtools/rehype-toc": "3.0.2",
    "@remix-run/node": "1.4.1",
    "@remix-run/react": "1.4.1",
    "@remix-run/serve": "1.4.1",
    "@remix-run/vercel": "1.4.1",
    "@tailwindcss/forms": "0.5.0",
    "airtable": "0.11.3",
    "clsx": "1.1.1",
    "contentful": "10.0.0-beta-v10.5",
    "dayjs": "1.11.1",
    "dotenv": "14.3.2",
    "pino": "7.10.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-typical": "^0.1.3",
    "rehype-slug": "5.0.1",
    "remix": "1.1.3",
    "snarkdown": "2.0.0"
