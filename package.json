{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "yarn build:css && remix build",
    "build:css": "tailwindcss -i ./app/styles/tailwind.css -o ./app/tailwind.css --minify",
    "dev": "concurrently \"yarn dev:css\" \"node -r dotenv/config node_modules/.bin/remix dev\"",
    "dev:css": "tailwindcss -i ./app/styles/tailwind.css -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "test": "jest"
  },
  "dependencies": {
    "@remix-run/react": "1.1.3",
    "@remix-run/serve": "1.1.1",
    "@remix-run/vercel": "1.1.1",
    "@tailwindcss/forms": "0.4.0",
    "airtable": "0.11.1",
    "clsx": "1.1.1",
    "contentful": "9.1.6",
    "dayjs": "1.10.7",
    "dotenv": "15.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remix": "1.1.3",
    "snarkdown": "2.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.16.8",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@drewster/eslint-config": "3.1.1",
    "@remix-run/dev": "1.1.3",
    "@tailwindcss/typography": "0.5.0",
