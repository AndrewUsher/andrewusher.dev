{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "module": "esnext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2019"
    ],
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "target": "ES2019",
    "strict": true,
    "baseUrl": ".",
    "noEmit": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "jsxImportSource": "react"
  }
}